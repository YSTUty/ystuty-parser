{
  "name": "ystuty-parser",
  "version": "0.0.15",
  "description": "",
  "author": "xTCry [Vladislav Kh] (https://github.com/xTCry/)",
  "private": true,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/YSTUty/ystuty-parser.git",
    "type": "git"
  },
  "homepage": "https://github.com/YSTUty/ystuty-parser#readme",
  "bugs": {
    "url": "https://github.com/YSTUty/ystuty-parser/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prettify": "prettier --write src/**/*",
    "prepare": "husky install",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.0",
    "axios": "^0.27.2",
    "body-parser": "1.19.1",
    "cheerio": "^1.0.0-rc.10",
    "cheerio-tableparser": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express-basic-auth": "^1.2.1",
    "fs-extra": "^10.0.0",
    "helmet": "^5.0.2",
    "ical-generator": "^3.4.0",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "nestjs-real-ip": "^2.0.0",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "standard-version": "^9.3.2",
    "swagger-stats": "^0.99.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/compression": "^1.7.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
